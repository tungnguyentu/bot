# Binance API Keys
# Create these at https://www.binance.com/en/my/settings/api-management
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# Telegram Settings
# Create a bot with @BotFather on Telegram to get the token
# Get your chat ID by messaging @userinfobot on Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Optional Settings
# Uncomment and modify these if you want to override default settings

# Trading Pairs (comma-separated)
# TRADING_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT

# Risk Settings
# LEVERAGE=20
# ACCOUNT_RISK_PER_TRADE=0.01
# MAX_DRAWDOWN_PER_TRADE=0.02

# Technical Indicator Parameters
# BB_PERIOD=14
# RSI_PERIOD=6
# RSI_OVERSOLD=25
# RSI_OVERBOUGHT=75
# MACD_FAST=5
# MACD_SLOW=13
# MACD_SIGNAL=1

# Stop Loss and Take Profit Settings
# SL_ATR_MULTIPLIER=1.5
# PARTIAL_TP_ATR_MULTIPLIER=1.0
# FULL_TP_ATR_MULTIPLIER=2.0
# PARTIAL_TP_SIZE=0.5
